def getFixFields(message):
    parts = message.split('')
    
    fields=[]
    
    for part in parts:
        if part!='\n':
            field={}
            (field['tag'],field['value']) = part.split('=')
            fields.append(field)
            
    return fields

def getFixField(tag, fields):   
    for field in fields:
        if field['tag']==35:
            print(field['value'])
    return 0

def run():
    file = open('C:\workspace\data\BVMF\MarketData_201105\MarketDataGather_201105_26a31\MarketDataGather_20110526.txt')
    for message in file:
        fields = getFixFields(message)
        #messageType = getFixField(35, fields)
        #print(messageType)
    file.close()
    
run()

